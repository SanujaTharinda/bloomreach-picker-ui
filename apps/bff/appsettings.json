{
  "ResourceSpace": {
    "BaseUrl": "http://localhost",
    "DefaultUser": "admin",
    "DefaultPageSize": 20,
    "MaxPageSize": 100,
    "TimeoutSeconds": 30
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Warning",
        "System.Net.Http": "Warning",
        "Microsoft.Extensions.Http": "Warning"
      }
    }
  },
  "AllowedHosts": "*"
}
